<?xml version='1.0' encoding='utf-8'?>
<widget id="com.marte.rataBBoy" version="0.1.0" xmlns="http://www.w3.org/ns/widgets"
    xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Rata B Boy</name>
    <description>
        Probando 1 2 3
    </description>
    <author email="marteindustria@gmail.com">
        Marte team
    </author>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="./public/logo.png" />
    <platform name="android">
        <icon src="./public/logo.png" density="ldpi" />
        <icon src="./public/logo.png" density="mdpi" />
        <icon src="./public/logo.png" density="hdpi" />
        <icon src="./public/logo.png" density="xhdpi" />
        <allow-intent href="market:*" />
    </platform>
    <preference name="android-minSdkVersion" value="16" />
    <preference name="android-targetSdkVersion" value="29" />
    <content src="index.html" />
</widget>